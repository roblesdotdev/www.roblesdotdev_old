{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ignore-path .ignore --ext js,jsx,ts,tsx .",
    "prettier": "prettier --ignore-path .ignore \"**/*.+(js|jsx|css|ts|tsx)\"",
    "format": "npm run prettier -- --list-different",
    "format:write": "npm run prettier -- --write"
  },
  "dependencies": {
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "prettier": "^2.8.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=18"
  }
}
